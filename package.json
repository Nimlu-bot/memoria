{
    "name": "memoria-monorepo",
    "version": "0.0.0",
    "private": true,
    "packageManager": "bun@1.2.16",
    "workspaces": [
        "apps/*"
    ],
    "scripts": {
        "dev": "concurrently \"bun run --cwd apps/back dev\" \"bun run --cwd apps/front start\"",
        "build-and-deploy": "bash scripts/build-and-deploy.sh"
    },
    "devDependencies": {
        "concurrently": "^9.2.1"
    }
}